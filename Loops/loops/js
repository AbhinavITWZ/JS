//map : a built in object in which key can be anything boolena,function,string,object anything
const mappy=new Map();
mappy.set(true,"boolean")
mappy.set(1,"One")
mappy.set(()=>{
    return this
},"function")
// console.log();


//for each ek loop h jo array ke andar h usko hum array ke through hi access kar sakte h
for (const [k,l] of mappy) {
    // console.log(k,l);
    
    
}
mappy.forEach((e,v)=>{
    console.log(e,v);
    
})
for (const key in mappy) {
//    console.log(key);
   
}
